			foreach ($this->funcs as $func) {
				if ($tok == T_STRING && is_int($func['toks'][0])) {
					continue;
				}
				if ($tok == T_OBJECT_OPERATOR && is_null($func['toks'][0])) {
					continue;
				}
				if ($tok == T_DOUBLE_COLON && is_null($func['toks'][0])) {
					continue;
				}

