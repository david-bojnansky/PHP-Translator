				if ($tok == T_SRTRING && is_null($func['toks'][0])) {
					if ($func['toks'][1] != $toks[ $i + 0 ][0] || !preg_match("~^{$func['name']}$~ADi", $con)) {
						continue;
					}
					if ($func['toks'][2] != $toks[ $i + 1 ][0] || $toks[ $i + 2 ][1] != '(') {
						continue;
					}
